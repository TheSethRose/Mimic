{
    "title": "Sam's Club - Get Order History",
    "description": "Login to Sam's Club and extract order history",
    "steps": [
        {
            "type": "setViewport",
            "width": 1280,
            "height": 800,
            "deviceScaleFactor": 1
        },
        {
            "type": "navigate",
            "url": "https://www.samsclub.com/login?redirectURL=/account/order-history"
        },
        {
            "type": "click",
            "target": "main",
            "selectors": [
                ["#email"],
                ["name/email"],
                ["aria/Email Address"],
                ["input[type='email']"]
            ]
        },
        {
            "type": "change",
            "value": "{{SAMS_EMAIL}}",
            "selectors": [
                ["#email"],
                ["input[type='email']"]
            ]
        },
        {
            "type": "click",
            "target": "main",
            "selectors": [
                ["#password"],
                ["name/password"],
                ["aria/Password"],
                ["input[type='password']"]
            ]
        },
        {
            "type": "change",
            "value": "{{SAMS_PASSWORD}}",
            "selectors": [
                ["#password"],
                ["input[type='password']"]
            ]
        },
        {
            "type": "click",
            "target": "main",
            "selectors": [
                ["button[type='submit']"],
                ["text/Sign In"],
                ["aria/Sign In"]
            ]
        },
        {
            "type": "waitForElement",
            "selectors": [
                ["[data-testid='order-history']"],
                ["text/Order History"],
                [".order-item"]
            ],
            "timeout": 15000
        }
    ],
    "env_vars": {
        "SAMS_EMAIL": "Sam's Club Email",
        "SAMS_PASSWORD": "Sam's Club Password"
    },
    "extract_prompt": "Extract the order history. For each order, capture: order_date, order_number, total, status, and items (array of item names). Return: {\"orders\": [{\"order_date\": \"...\", \"order_number\": \"...\", \"total\": \"$X.XX\", \"status\": \"...\", \"items\": [\"item1\", \"item2\"]}]}"
}
